function OS_7() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('H3').activate();
  spreadsheet.duplicateActiveSheet();
  spreadsheet.getRange('G2').activate();
  spreadsheet.getActiveSheet().setName('E7');
  spreadsheet.getCurrentCell().setValue('E7');
  spreadsheet.getRange('H2').activate();
  spreadsheet.getCurrentCell().setValue('SALGADOS ASSADOS (MASSA PODRE)');
  spreadsheet.getActiveRangeList().setWrapStrategy(SpreadsheetApp.WrapStrategy.CLIP);
  spreadsheet.getRange('H3').activate();
  var criteria = SpreadsheetApp.newFilterCriteria()
  .whenTextStartsWith('E7')
  .build();
  spreadsheet.getActiveSheet().getFilter().setColumnFilterCriteria(8, criteria);
};

