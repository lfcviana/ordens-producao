function EXE_Ordens_setoriais() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.setActiveSheet(spreadsheet.getSheetByName('TOR'), true); 
  OS_0();
  OS_1();
  OS_2();
  OS_3();
  OS_4();
  OS_5();
  OS_6();
  OS_7();
  OS_F7();
  OS_N7();
  OS_8();
  OS_9();
};



function OS_0() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('H3').activate();
  spreadsheet.duplicateActiveSheet();
  spreadsheet.getRange('G2').activate();
  spreadsheet.getCurrentCell().setValue('E0');
  spreadsheet.getRange('H2').activate();
  spreadsheet.getActiveSheet().setName('E0');
  spreadsheet.getCurrentCell().setValue('PÃES');
  spreadsheet.getRange('H3').activate();  
  var criteria = SpreadsheetApp.newFilterCriteria()
  .whenTextStartsWith('E0')
  .build();
  spreadsheet.getActiveSheet().getFilter().setColumnFilterCriteria(8, criteria);
};

function OS_1() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('H3').activate();
  spreadsheet.duplicateActiveSheet();
  spreadsheet.getRange('G2').activate();
  spreadsheet.getCurrentCell().setValue('E1');
  spreadsheet.getRange('H2').activate();
  spreadsheet.getActiveSheet().setName('E1');
  spreadsheet.getCurrentCell().setValue('TORTAS SIMPLES E CAKES');
  var criteria = SpreadsheetApp.newFilterCriteria()
  .whenTextStartsWith('E1')
  .build();
  spreadsheet.getActiveSheet().getFilter().setColumnFilterCriteria(8, criteria);
};

